              </div>
            </div>

            <div class="row form-group">
              <div class="col-md-12">
                <label class="text-black" for="message">Message</label>
                <textarea placeholder="Personal message..." required name="message" id="messageForm" cols="30" rows="7" class="form-control"></textarea>
              </div>
            </div>

            <div class="row form-group">
              <div class="col-md-12">
                <%= invisible_recaptcha_tags text: "Send Message", class: "btn btn-primary btn-md text-white" %>
              </div>
            </div>
          <% end %>
        </div>

